location /phpmyadmin/ {
            index            index.php;
            proxy_set_header X-Forwarded-Proto https;            
            proxy_pass       http://phpmyadmin:5000/;
            proxy_redirect   /index.php  /phpmyadmin/index.php;
        }

  
[meta]
  dir = "/var/lib/influxdb/meta"

[data]
  dir = "/var/lib/influxdb/data"
  engine = "tsm1"
  wal-dir = "/var/lib/influxdb/wal"

[http]
  enabled = true
  auth-enabled = true
  bind-address = ":8086"
  https-enabled = false